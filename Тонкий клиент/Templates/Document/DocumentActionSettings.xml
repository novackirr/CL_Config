	<action type="CopyDocument">
		<flows>
			<flow name="*">
				<field name="|Key" action="doNotCopy" />
				<field name="|Document|Число_регномера" action="doNotCopy" />
				<field name="|Document|Регистрационный_номер" action="doNotCopy" />
			</flow>
			<flow name="Позиции плана закупок 223ФЗ" copyLinkedDocuments="false" isNeedLinks="false" >
				<field name="|Document|Дата_регистрации" action="setCurrentDate" />
				<field name="|Document|Статус" action="setValue" value="Черновик" />
				<field name="|Document|Номер_редакции" action="setValue" value="1" />
				<field name="|Document|ИД_первой_редакции" action="doNotCopy" />
				<field name="|Document|ИД_в_ЕИС" action="doNotCopy" />
				<field name="|Document|ИД_позиции_плана" action="doNotCopy" />
				<field name="|Document|Инициатор|Инициатор" action="setCurrentUser"/>
				<field name="|Document|Ответственный_исполнитель|Ответственный_исполнитель" action="doNotCopy" />
				<field name="|Document|Ответственный_исполнитель|Ответственный_исполнитель_ФИО" action="doNotCopy" />
			</flow>
			<flow name="Заявки на закупку" copyLinkedDocuments="true" >
				<field name="|Document|Дата_регистрации" action="setCurrentDate" />
				<field name="|Document|Статус" action="setValue" value="Черновик" />
			</flow>
			<flow name="Контракты" copyLinkedDocuments="true" >
				<field name="|Document|Дата_регистрации" action="setCurrentDate" />
				<field name="|Document|Статус" action="setValue" value="Черновик" />
			</flow>
			<flow name="Позиции">
				<field name="|Document|ИД_заявки" action="setParentDocKey" />
				<field name="|Document|Статус" action="setValue" value="Черновик" />
				<field name="|Document|ИД_позиции_ПГ" action="doNotCopy" />
				<field name="|Document|ИД_контракта" action="doNotCopy" />
				<field name="|Document|ИД_позиции_ПЗ" action="doNotCopy" />
				<field name="|Document|ИД_позиции_ПЗ" action="doNotCopy" />
				<field name="|Document|Тип_особой_закупки_код" action="doNotCopy" />
				<field name="|Document|Тип_особой_закупки" action="doNotCopy" />
			</flow>
		</flows>
	</action>
	<action type="CreateFromLink">
		<flows>
