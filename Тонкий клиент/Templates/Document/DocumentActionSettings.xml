				<field name="|Key" action="doNotCopy" />
				<field name="|Document|Число_регномера" action="doNotCopy" />
				<field name="|Document|Регистрационный_номер" action="doNotCopy" />
			</flow>
			<flow name="Позиции">
				<attachments>
					<category name="Присоединенные файлы" action="doNotCopy"/>
				</attachments>
				<!-- <links>
					<link flow="Позиции плана-графика" action="doNotCopy" />
				</links> -->
				<field name="|Key" action="doNotCopy" />
				<field name="|Document|Число_регномера" action="doNotCopy" />
				<field name="|Document|Регистрационный_номер" action="doNotCopy" />
				<field name="|Document|ИД_заявки" action="setParentDocKey" />
				<field name="|Document|Статус" action="setValue" value="Позиция контракта" />
				<field name="|Document|ИД_позиции_ПЗ" action="doNotCopy" />
			 	<field name="|Document|ИД_контракта" action="doNotCopy" /> 
			
			</flow>
			<flow name="Контракты">
				<attachments>
					<category name="Присоединенные файлы" action="doNotCopy"/>
				</attachments>
				
				<field name="|Key" action="doNotCopy" />
				<field name="|Document|Число_регномера" action="doNotCopy" />
				<field name="|Document|Регистрационный_номер" action="doNotCopy" />
				<field name="|Document|Номер_редакции" action="doNotCopy" /> 
			
			</flow>
			
			
		
						
		</flows>
	</action>
</actions>
